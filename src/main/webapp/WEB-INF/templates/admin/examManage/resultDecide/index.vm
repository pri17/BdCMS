<script>

    //点击医生的显示按钮
    getNavEle( '#showAutoDoctor').click(function () {

        getNavEle( '#autoDoctorName').attr('disabled', false);
        //自身变为不可用,开放修改按钮
        getNavEle( '#showAutoDoctor').attr('disabled', true);
        getNavEle( '#changeAutoDoctor').attr('disabled', false);

    });

    //点击修改按钮
    getNavEle( '#changeAutoDoctor').click(function () {
        var doctorName = getNavEle( '#autoDoctorName').val();

        if (doctorName.length <= 0) {

            getNavEle( this).alertmsg('error', '请输入审核医生姓名!', {
                displayMode: 'none',
                displayPosition: '',
                okName: '确定',
                title: '错误提示'
            })

        } else {

            $.post("#U('updateAutoDoctor')", {doctorName: doctorName}, function (data) {

                data = $.parseJSON(data);

                if (data) {
                    if (data.statusCode == 200) {

                        getNavEle( this).alertmsg('ok', '修改审核医生成功');

                        //将按钮还原
                        getNavEle( '#autoDoctorName').attr('disabled', true);
                        //自身变为不可用,开放修改按钮
                        getNavEle( '#showAutoDoctor').attr('disabled', false);
                        getNavEle( '#changeAutoDoctor').attr('disabled', true);

//                        var examMemberId = data.message;

                    } else if (data.statusCode == 300) {
                        getNavEle( this).alertmsg('error', data.message, {
                            displayMode: 'none',
                            displayPosition: '',
                            okName: '确定',
                            title: '错误提示'
                        })
                    }

                }


            });

        }


    })


    getNavEle( document).ready(function () {

        //全选或全不选
        getNavEle( "#rdAllIds").click(function () {
            if (this.checked) {
                getNavEle( "#rdList [name=rdIds]:checkbox").attr("checked", true);
            } else {
                getNavEle( "#rdList [name=rdIds]:checkbox").attr("checked", false);
            }
        });

        getNavEle( "#rdList [name=rdIds]:checkbox").click(function () {

            if (getNavEle( this).attr("checked") == 'checked') {

                getNavEle( this).attr("checked", true);

            } else {

                getNavEle( this).attr("checked", false);
            }

            allchk();

        });

    });


    function allchk() {
        var chknum = getNavEle( "#rdList [name=rdIds]:checkbox").size();//选项总个数
        var chk = 0;
        getNavEle( "#rdList [name=rdIds]:checkbox").each(function () {
            if (getNavEle( this).attr("checked") == 'checked') {
                chk++;
            }
        });

        if (chknum == chk) {//全选
            getNavEle( "#rdAllIds").attr("checked", true);
        } else {//不全选
            getNavEle( "#rdAllIds").attr("checked", false);
        }
    }



