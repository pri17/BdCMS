        var verifyState = getNavEle( '.rdcontent #verifyState').val();
        window.open("#U("/admin/examManage/resultDecide/exportFindExcel")" + "?pageCurrent="
                + pageCurrent + "&pageCount=" + pageCount + "&pageSize=" + pageSize+ "&areaId=" + areaId+
                "&rdOpenCategoryLevelTwoId=" + rdOpenCategoryLevelTwoId+ "&memberName=" + memberName+ "&idCardNum=" + idCardNum+
                "&examNumber=" + examNumber+"&workUnit="+ workUnit+ "&beginTime="+ beginTime+
                "&endTime=" + endTime+ "&verifyConclusion=" + verifyConclusion+ "&verifyState=" + verifyState);
    });

    //行业类别二级选项卡
    var resultDecideAddOption = function (tid) {
        getNavEle( ".rdCategoryLevelTwoClass").remove();
        var pid = getNavEle( "#rdOpenCategory").val();
        $.getJSON("#U("/admin/examBusiness/examAdd/getCategoryByPid")",
                {
                    pid: pid
                },
                function (data) {
                    if (data.categoryList.length > 0) {
                        var html = "";
                        for (var i = 0; i < data.categoryList.length; i++) {
                            var category = data.categoryList[i];
                            if (category.id == tid) {
                                html += "<option class='rdCategoryLevelTwoClass' value='" + category.id + "' selected>" + category.categoryName + "</option>";
                            } else {
                                html += "<option class='rdCategoryLevelTwoClass' value='" + category.id + "'>" + category.categoryName + "</option>";
                            }
                        }
                        getNavEle( "#rdOpenCategoryLevelTwo").append(html);
                    }
                }
        );
    }
    //点击全选按钮
    getNavEle( "#rdAllIds").on("click", function (e) {
        if (getNavEle( this).attr("checked")) {
            getNavEle( "[type='checkbox']").each(function () {
                getNavEle( this).attr("checked", 'true');
//                examIds += getNavEle( this).val()+",";
            });
        } else {
            getNavEle( "[type='checkbox']").each(function () {
                getNavEle( this).removeAttr("checked");
            });
        }
    });

    //判断审核
    var judgment = function () {
        var examIds = "";
        //查出所有已选择的体检数据
        getNavEle( "tbody [name='rdIds']").each(function () {
            if (getNavEle( this).attr("checked")) {
                examIds += getNavEle( this).val() + ",";
            }
        });
        if (examIds == '') {
            getNavEle( this).alertmsg('error', "未选择审核内容");
            return;
        }
        getNavEle( "#judgmentResult").attr("data-url", "#U("/admin/examManage/resultDecide/batchAudit?examIds=")" + examIds);
    };
    //点击审核按钮回调
    function judgmentCallback() {
        getNavEle( "#reloadSearch").click();
    }
    ;

    //撤销审核
    var cancelVerify = function () {

        var examIds = "";
        //查出所有已选择的体检数据
        getNavEle( "tbody [name='rdIds']").each(function () {
            if (getNavEle( this).attr("checked")) {
                examIds += getNavEle( this).val() + ",";
            }
        });
        if (examIds == '') {
            getNavEle( this).alertmsg('error', "未选择需要撤销审核的用户");
            return;
        }
        $.getJSON("#U("/admin/examManage/resultDecide/cancelChecked")",
                {
                    examIds: examIds
                },
                function (data) {
                    if (data.statusCode == 300) {
                        getNavEle( this).alertmsg('error', data.message);
                    } else {
                        getNavEle( this).alertmsg('ok', data.message);
                        getNavEle( "#reloadSearch").click();
                    }
                });

    }


